{
  "name": "Scribe",
  "description": "DM Tools",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/luetkemj/scribe"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/luetkemj/scribe/raw/master/LICENSE"
  },
  "scripts": {
    "clean:build": "rimraf build && rimraf public",
    "clean:node_modules": "rimraf node_modules",
    "clean": "npm run clean:build && npm run clean:node_modules",
    "reinstall": "npm run clean && npm install",
    "start": "./node_modules/.bin/nodemon --exec NODE_ENV=development ./node_modules/.bin/babel-node ./server/index.js",
    "lint:sass-lint": "sass-lint -c ./.sass-lint.yml './app/**/*.scss' -v",
    "lint:eslint": "eslint .",
    "lint": "npm run lint:sass-lint && npm run lint:eslint",
    "build:client": "NODE_ENV=production webpack --config ./webpack.config.js --progress --profile --colors",
    "build:server": "./node_modules/.bin/babel -d ./build ./server -s",
    "build": "npm run lint && npm run clean:build && npm run build:server && npm run build:client",
    "production-build": "npm run build && npm run clean:node_modules && npm install --production",
    "production": "NODE_ENV=production node ./build/index.js",
    "test:mocha": "NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register --recursive test/**",
    "test:watch": "NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register --recursive --watch test/**",
    "test": "npm run lint && npm run test:mocha",
    "test-app": "TEST_APP=true NODE_ENV=development ./node_modules/.bin/babel-node ./server/index.js"
  },
  "dependencies": {
    "async": "2.1.5",
    "body-parser": "1.17.0",
    "connect-history-api-fallback": "1.3.0",
    "cookie-parser": "1.4.3",
    "debug-caller": "2.2.0",
    "express": "4.15.0",
    "express-jwt": "5.1.0",
    "glob": "7.1.1",
    "jsonwebtoken": "7.3.0",
    "lodash": "4.17.4",
    "md5": "2.2.1",
    "mongoose": "4.8.5",
    "normalize.css": "5.0.0",
    "react": "15.4.2",
    "react-addons-create-fragment": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.8",
    "react-textarea-autosize": "4.0.5",
    "redux": "3.6.0",
    "redux-actions": "1.2.2",
    "redux-logger": "2.8.2",
    "redux-thunk": "2.2.0",
    "request": "2.79.0",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-strict-mode": "6.22.0",
    "babel-preset-env": "1.2.0",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-2": "6.22.0",
    "css-loader": "0.26.2",
    "eslint": "3.16.1",
    "eslint-config-airbnb": "14.1.0",
    "eslint-loader": "1.6.3",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "fetch-mock": "5.9.4",
    "html-webpack-plugin": "2.28.0",
    "mocha": "3.2.0",
    "moment": "2.17.1",
    "node-sass": "4.5.0",
    "nodemon": "1.11.0",
    "react-hot-loader": "3.0.0-beta.0",
    "redux-mock-store": "1.2.2",
    "rimraf": "2.6.1",
    "sass-lint": "1.10.2",
    "sass-loader": "6.0.2",
    "should": "11.2.0",
    "style-loader": "0.13.2",
    "svg-url-loader": "2.0.2",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.1",
    "webpack-hot-middleware": "2.17.1"
  }
}
